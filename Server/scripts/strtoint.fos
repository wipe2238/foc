// mapper, client and server script
bool StrToInt(string@ s, uint64& inout val)
{
	int temp=0;
	if(!StrToInt(s,temp)) return false;
	val=temp;
	return true;
}

bool StrToInt(string@ s, uint& inout val)
{
	int temp=0;
	if(!StrToInt(s,temp)) return false;
	val=temp;
	return true;
}

